# 配置指定Github平台使用的密钥
Host github.com
    HostName github.com
	Port 22
    AddKeysToAgent yes
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/github-id-rsa
	
# 配置指定Gitee平台使用的密钥
Host gitee.com
    HostName gitee.com
	Port 22
    AddKeysToAgent yes
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/gitee-id-rsa
	
# 配置指定本地Gitlab平台使用的密钥
Host yhm.gitlab.com
    HostName 10.100.1.1
	Port 50022
    AddKeysToAgent yes
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/10.100.1.1-gitlab-id-rsa